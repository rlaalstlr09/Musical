<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.musical.dao.IHallDao">


<select id="select_hall" resultType="com.musical.dto.HallDto">
select * from hall where hall_id=#{param1} 
</select>


<select id="hall_list_venue" resultType="com.musical.dto.HallDto">
select * from hall where venue_id=#{param1} 
</select>


</mapper>









